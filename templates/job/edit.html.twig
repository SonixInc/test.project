{% extends 'base.html.twig' %}

{% block body %}
    <div class="card">
        <div class="card-header">Job edit</div>
        <div class="card-body">
            {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
            {{ form_widget(form) }}

            <div class="form-group">
                <button type="submit" class="btn btn-primary">Edit</button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}

